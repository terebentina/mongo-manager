{
  "name": "mongo-syllabus",
  "version": "0.5.0",
  "description": "Mongodb web based admin interface written in React.JS",
  "main": "main.js",
  "bin": {
    "msyll": "cli.js"
  },
  "scripts": {
    "start": "node server.js",
    "build": "webpack --progress --colors --config webpack.config.prod.js",
    "build-tests": "webpack --config webpack.config.test.js",
    "test": "testem",
    "travis": "testem ci"
  },
  "author": "Dan Caragea",
  "license": "MIT",
  "dependencies": {
    "@terebentina/react-popover": "^1.0.4",
    "axios": "^0.9.1",
    "classnames": "^2.2.3",
    "ejs": "^2.4.1",
    "js-beautify": "^1.6.2",
    "lodash": "^4.6.1",
    "mongodb": "^2.1.7",
    "normalize.css": "^3.0.3",
    "prismjs": "^1.4.1",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "restify": "^4.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-color-converter": "^1.0.2",
    "css-loader": "^0.23.1",
    "enzyme": "^2.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "ghooks": "^1.0.3",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.8.2",
    "postcss-scss": "^0.1.7",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^0.14.7",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.1.1",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.0.2",
    "sinon": "github:sinonjs/sinon",
    "style-loader": "^0.13.0",
    "tape": "^4.5.1",
    "testem": "^1.5.0",
    "validate-commit-msg": "^2.4.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    }
  }
}
